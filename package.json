{
  "name": "neoarchive",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "start": "node server.js",
    "deploy": "npm install && npm run build && pm2 start ecosystem.config.cjs --env production",
    "migrate:export": "node migration-export.js",
    "migrate:import": "node migration-import.js",
    "migrate:import:pg": "node migration-import-postgres.js",
    "migrate:all": "npm run migrate:export && npm run migrate:import",
    "migrate:postgres": "npm run migrate:export && npm run migrate:import:pg"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.87.2",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "lucide-react": "^0.561.0",
    "pg": "^8.14.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0"
  }
}
